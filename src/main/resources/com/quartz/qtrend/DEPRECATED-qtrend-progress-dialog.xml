<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
    <bean   id="ProgressDialog" class="com.quartz.qtrend.ui.ProgressDialog">
        <constructor-arg    index="0" type="javax.swing.JFrame" ref="QTrendFrame"/>
        <property name="title" value="Progress..."/>
        <property name="progressBar.indeterminate" value="true"/>
        <property name="progressBar.stringPainted" value="true"/>
    </bean>
</beans>